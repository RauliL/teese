{
  "name": "teese",
  "version": "1.0.0",
  "description": "Minimal Kanban board Web application",
  "author": "Rauli Laine <rauli.laine@iki.fi>",
  "license": "MIT",
  "keywords": [
    "kanban",
    "self-hosted"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/teese.git"
  },
  "bugs": {
    "url": "https://github.com/RauliL/teese/issues"
  },
  "homepage": "https://github.com/RauliL/teese",
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "build": "webpack-cli",
    "watch": "webpack-cli --watch",
    "start": "node './lib/index.js'",
    "start:dev": "nodemon --watch './lib' './lib/index.js'",
    "lint": "eslint './src/**/*.ts'",
    "style:check": "prettier --check './src/**/*.{ts,tsx}'",
    "style:write": "prettier --write './src/**/*.{ts,tsx}'",
    "clean": "rimraf './lib' './public/static'",
    "prepack": "yarn clean && env NODE_ENV=production yarn build"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "eslint": "^7.24.0",
    "formik": "^2.2.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-showdown": "^2.3.0",
    "rimraf": "^3.0.2",
    "swr": "^0.5.5",
    "ts-loader": "^8.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@varasto/storage": "^2.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  }
}
